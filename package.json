{
  "name": "clusterix",
  "version": "0.2.0",
  "description": "Node module for cluster node management",
  "type": "module",
  "main": "commonjs/entry.js",
  "exports": {
    ".": {
      "require": "./commonjs/entry.js",
      "default": "./esm/entry.mjs"
    }
  },
  "module": "./esm/entry.mjs",
  "scripts": {
    "lint": "eslint 'src/*.js' 'test/*.cjs'",
    "build": "rollup -c rollup.config.js",
    "pretest": "npm run lint && npm run build",
    "test": "mocha test/index.cjs",
    "cover": "nyc npm test",
    "coveralls": "npm run cover && nyc report --reporter=text-lcov | coveralls",
    "prepare": "npm run build"
  },
  "files": [
    "cjs",
    "esm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perrin4869/clusterix.git"
  },
  "keywords": [
    "cluster",
    "redis"
  ],
  "author": "Julian Grinblat <julian@dotcore.co.il>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perrin4869/clusterix/issues"
  },
  "homepage": "https://github.com/perrin4869/clusterix#readme",
  "dependencies": {
    "delay": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "@babel/register": "^7.8.0",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.9",
    "env-test": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-mocha": "^6.2.2",
    "ioredis": "^4.14.1",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "redis-pdel": "^1.0.3",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "sinon": "^8.0.4",
    "sinon-chai": "^3.4.0"
  }
}
